<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sayho.workoutcalendar.dao.WorkoutRecordDao">
  <insert id="insertWorkoutRecord" parameterType="java.util.Map">
    INSERT INTO ICTWORKOUTRECORD(USR_ID, REG_DT, WO_DT, PART_MAIN_COD, WO_COD, REST_TIME_SEC, SETS, REPS, WEIGHT, VOLUME)
    VALUES(#{usrId}, TO_CHAR(NOW(), 'YYYYMMDD'), #{woDt}, #{partMainCod}, #{woCod}, #{restTimeSec}, #{sets}, #{reps}, #{weight}, #{volume})
  </insert> 
